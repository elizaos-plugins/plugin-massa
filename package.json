{
    "name": "@elizaos/plugin-massa",
    "version": "0.1.8+build.1",
    "main": "dist/index.js",
    "type": "module",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@elizaos/core": "workspace:*",
        "@massalabs/massa-web3": "^5.0.1-dev",
        "tsup": "8.3.5"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "lint": "eslint . --fix"
    },
    "peerDependencies": {
        "whatwg-url": "7.1.0"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "MASSA_PRIVATE_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Massa private key is required"
            },
            "MASSA_RPC_URL": {
                "type": "string",
                "minLength": 1,
                "description": "Massa RPC URL is required"
            }
        }
    }
}
